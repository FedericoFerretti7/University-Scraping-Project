l.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-dc467b08-afb1-4fa9-939f-6fe15be87a8b button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","    </div>\n","  </div>\n"]},"metadata":{},"execution_count":17}]},{"cell_type":"markdown","source":["Same situation for cluster 3 which, however, is characterized by having the weights in which the perception of corruption is lower than that of the countries in the previous cluster."],"metadata":{"id":"Uu-BZmaqx_xR"}},{"cell_type":"code","source":["dh_2013_std_norm[dh_2013_std_norm['Cluster']==4]"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":585},"id":"a80F293YyBW3","executionInfo":{"status":"ok","timestamp":1694074639670,"user_tz":-120,"elapsed":128,"user":{"displayName":"Ilaria Salvatori","userId":"04565068118230364096"}},"outputId":"fdb9092a-d052-48d6-d71d-477fbd5d75a6"},"execution_count":18,"outputs":[{"output_type":"execute_result","data":{"text/plain":["     index    Country name  Log GDP per capita  Social support  \\\n","5        5       Australia            1.278421        1.084950   \n","6        6         Austria            1.392744        1.276570   \n","8        8         Bahrain            1.319757        0.690917   \n","11      11         Belgium            1.306426        0.916257   \n","22      22          Canada            1.277643        1.156210   \n","33      33         Denmark            1.366273        1.408718   \n","40      40         Finland            1.250410        1.197274   \n","44      44         Ge